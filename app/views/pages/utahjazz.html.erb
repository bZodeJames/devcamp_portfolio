<div class="inner cover">
    <h1 class="cover-heading">Utah Jazz Tweets</h1>

    <div class="row">
        <% @tweets.each do |tweet| %>
            <div class="col-md-6">
                <div class="utahjazz">
                    <div class="card">
                        <%= tweet_parser tweet %>
                    </div>
                </div>
            </div>
        <% end %>
    </div>
</div>